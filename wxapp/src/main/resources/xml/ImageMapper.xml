<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.senup.wxapp.mapper.ImageMapper">

    <select id="selectByArticleId"  resultType="com.senup.wxapp.entity.Image" >
        SELECT image.*
        from image
        where art_id = #{artId}
    </select>
</mapper>
