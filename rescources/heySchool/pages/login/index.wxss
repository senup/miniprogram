@import "../authorization/index.wxss";

.page_ft view {
    height: 50rpx;
    line-height: 50rpx;
}

.text-area-note {
    font-size: 24rpx;
}

checkbox .wx-checkbox-input {
    width: 30rpx;
    height: 30rpx;
    margin-bottom: 6rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    width: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 20rpx;
    color: #666;
    background: transparent;
    transform: translate(-50%,-50%) scale(1);
}